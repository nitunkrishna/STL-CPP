#include"bits/stdc++.h"

using namespace std;

int main()
{
    vector<int>v={5, 3, 7, 9, 1, 4, 8, 2, 5, 8, 9, 5};
    
    // Maximum element of a vector
    vector<int>::iterator it_max=max_element(v.begin(), v.end());
    cout << "Maximum element= " << *it_max << endl;

    // Maximum element of a vector from index-0 to index-3
    vector<int>::iterator it_max03=max_element(v.begin(), v.begin()+4);
    cout << "Maximum element within index 0 to 3= " << *it_max03 << endl;

    // Index of maximum element of a vector
    int max_index=max_element(v.begin(), v.end()) -v.begin();
    cout << "Index of maximum element= " << max_index << endl;

    // Minimum element of a vector
    vector<int> :: iterator it_min=min_element(v.begin(), v.end());
    cout << "Minimum element= " << *it_min << endl;

    // Minimum element of a vector from index-0 to index-3
    vector<int> :: iterator it_min03=min_element(v.begin(), v.begin()+4);
    cout << "Minimum element within index 0 to 3= " << *it_min03 << endl;

    // Index of minimum element of a vector
    int min_index=min_element(v.begin(), v.end()) -v.begin();
    cout << "Index of minimum element= " << min_index << endl;

    return 0;
}
